#!/usr/bin/env php
<?php

  require __DIR__.'/../vendor/autoload.php';

  use Symfony\Component\Console\Application;

  $application = new Application('Shade', '0.1-dev');
  $application->add(new ActiveCollab\Shade\Command\BooksCommand());
  $application->add(new ActiveCollab\Shade\Command\ProjectCommand());
  $application->run();