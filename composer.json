{
    "name": "activecollab/shade",
    "description": "Build help portals with ease",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-gettext": "*",
        "ext-json": "*",
        "symfony/console": "^4.0",
        "herrera-io/phar-update": "~1.0",
        "michelf/php-markdown": "~1.8",
        "smarty/smarty": "^3.1",
        "jbroadway/urlify": "~1.0",
        "phine/phar": "~1.0",
        "voku/simple_html_dom": "~4.2",
        "php-di/php-di": "^6.0",
        "scrivo/highlight.php": "^9.13"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\Shade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveCollab\\Shade\\Test\\": "test/src"
        }
    }
}
